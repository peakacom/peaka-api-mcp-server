{
  "name": "@peaka/mcp-server-peaka-api",
  "version": "0.0.2",
  "description": "MCP Server Implementation for Peaka API",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-server-peaka-api": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "access": "public",
  "scripts": {
    "postbuild": "shx chmod +x dist/*.js",
    "start": "node --watch dist/index.js",
    "build": "tsup src/index.ts --dts --format esm --external xdg-app-paths --external which && shx cp src/api.json dist/api.json",
    "check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peakacom/peaka-api-mcp-server.git"
  },
  "keywords": [
    "peaka",
    "mcp",
    "ai",
    "api"
  ],
  "author": "Mevlut Can TUNA",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peakacom/peaka-api-mcp-server/issues"
  },
  "homepage": "https://github.com/peakacom/peaka-api-mcp-server#readme",
  "devDependencies": {
    "@types/node": "^22.15.24",
    "shx": "^0.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "fastmcp": "^2.1.4"
  }
}
